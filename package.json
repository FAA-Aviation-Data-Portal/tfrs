{
  "name": "@faa-aviation-data-portal/tfrs",
  "description": "Fetch temporary flight restrictions and related data from https://tfr.faa.gov",
  "version": "0.1.0",
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "superagent": "^8.1.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.1.0"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.10"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ]
    }
  },
  "files": [
    "index.js",
    "license",
    "readme.md"
  ],
  "keywords": [
    "FAA",
    "NOTAM",
    "TFR",
    "aviation",
    "flight",
    "hazards",
    "restrictions",
    "warnings"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": "FAA-Aviation-Data-Portal/tfrs",
  "scripts": {
    "test": "eslint *.js && nyc mocha test.js"
  }
}
